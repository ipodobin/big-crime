{
  "Version":"2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadWriteList",
      "Effect": "Allow",
      "Principal": "*",
      "Action": ["s3:GetObject","s3:PutObject","s3:ListBucket"],
      "Resource": [
        "arn:aws:s3:::$AWS_ACCOUNT_ID-$LANDING_ZONE_BUCKET_NAME",
        "arn:aws:s3:::$AWS_ACCOUNT_ID-$LANDING_ZONE_BUCKET_NAME/*"
      ]
    }
  ]
}